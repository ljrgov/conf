#!name=阿里云 DNS
#!desc=阿里云 DoH (DNS over HTTPS) 查询，支持 IPv4 和 IPv6
#!category=📘功能模块
#!arguments=HOST: 223.5.5.5, DOMAIN: *, DID: IOS, UID: 阿里云账户ID, ID: AccessKey ID, SECRET: AccessKey Secret

[Host]
{{{DOMAIN}}} = script:AliyunDNS

[Script]
AliyunDNS = type=dns,script-path=https://raw.githubusercontent.com/ljrgov/conf/refs/heads/main/script/alidns.js,argument={"HOST":"{{{HOST}}}","DID":"{{{DID}}}","UID":"{{{UID}}}","ID":"{{{ID}}}","SECRET":"{{{SECRET}}}"}