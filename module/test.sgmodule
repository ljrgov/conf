#!name=机场信息
#!desc=流量信息/智能提醒/到期日期
#!system=ios
#!category=📕面板模块
#!author=cc63,基于此作者修改
#!arguments=url1:enable1:true, 机场1订阅url编码, title1:机场1名字, icon1:tornado, color1:#DF4688, enable2:true, url2:机场2订阅url编码, title2:机场2名字, icon2:waveform, color2:#EF6D20, enable3:true, url3:机场3订阅url编码, title3:机场3名字, icon3:waveform, color3:#DAA520
#!arguments-desc=- enable：是否启用该机场，true 启用，false 不启用; \n- URL：订阅链接：url编码后的订阅链接; \n- title：机场的名称; \n- icon：面板处显示的图标,sf symbols复制图标名粘贴; \n- url编码：网址urlencoder.org 或通过快捷指令 \n- color：颜色代码参考^_^ 






[Panel]
Sub-info = script-name=1号机场,update-interval=43200,enabled={{{enable1}}}
Sub-info = script-name=2号机场,update-interval=43200,enabled={{{enable2}}}
Sub-info = script-name=3号机场,update-interval=43200,enabled={{{enable3}}}

[Script]
1号机场 = type=generic,timeout=5,script-path=https://raw.githubusercontent.com/ljrgov/conf/main/script/panel/airport.js,script-update-interval=86400,argument="url={{{url1}}}&title={{{title1}}}&icon={{{icon1}}}&color={{{color1}}}",enabled={{{enable1}}}
2号机场 = type=generic,timeout=5,script-path=https://raw.githubusercontent.com/ljrgov/conf/main/script/panel/airport.js,script-update-interval=86400,argument="url={{{url2}}}&title={{{title2}}}&icon={{{icon2}}}&color={{{color2}}}",enabled={{{enable2}}}
3号机场 = type=generic,timeout=5,script-path=https://raw.githubusercontent.com/ljrgov/conf/main/script/panel/airport.js,script-update-interval=86400,argument="url={{{url3}}}&title={{{title3}}}&icon={{{icon3}}}&color={{{color3}}}",enabled={{{enable3}}}